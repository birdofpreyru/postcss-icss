{
  "name": "postcss-icss",
  "version": "2.0.0",
  "description": "Postcss plugin to process css modules and extract tokens",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --out-dir lib src",
    "test": "jest --coverage",
    "precommit": "lint-staged",
    "prepublish": "yarn test && yarn run build"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "es6": true
    },
    "extends": "eslint:recommended"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ]
    ]
  },
  "repository": "css-modules/postcss-icss",
  "keywords": [
    "css-modules",
    "postcss",
    "css",
    "icss",
    "postcss-plugin"
  ],
  "author": "Alexey Litvinov",
  "license": "MIT",
  "dependencies": {
    "icss-replace-symbols": "^1.0.2",
    "icss-utils": "^2.1.0",
    "lodash.foreach": "^3.0.3",
    "postcss": "^5.0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.5.1",
    "eslint": "^3.19.0",
    "husky": "^0.13.3",
    "jest": "^20.0.4",
    "lint-staged": "^3.5.0",
    "prettier": "^1.3.1",
    "strip-indent": "^2.0.0"
  }
}
